          "parameters": [
            {
              "name": "frequency",
              "value": "={{ $json.frequency || 'quarterly' }}"
            }
          ]
        },
        "options": {}
      },
      "name": "Get All Fundamentals",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.1,
      "position": [850, 900],
      "description": "Get all fundamental financial statements. Required: symbol. Optional: frequency ('annual' or 'quarterly', default: 'quarterly')"
    },
    {
