{
  "name": "Market Data API - n8n HTTP Requests",
  "nodes": [
    {
      "parameters": {
        "url": "https://market-data-c60q.onrender.com/",
        "method": "GET",
        "options": {}
      },
      "name": "API Root Info",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.1,
      "position": [250, 300],
      "description": "Get API information"
    },
    {
      "parameters": {
        "url": "https://market-data-c60q.onrender.com/health",
        "method": "GET",
        "options": {}
      },
      "name": "Health Check",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.1,
      "position": [450, 300],
      "description": "API health status check"
    },
    {
      "parameters": {
        "url": "https://market-data-c60q.onrender.com/api/v1/stock/{{ $json.symbol }}/history",
        "method": "GET",
        "sendQuery": true,
        "queryParameters": {
          "parameters": [
            {
              "name": "start_date",
              "value": "={{ $json.start_date }}"
            },
            {
              "name": "end_date",
              "value": "={{ $json.end_date }}"
            }
          ]
        },
        "options": {}
      },
      "name": "Get Stock History",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.1,
      "position": [250, 500],
      "description": "Get historical stock price data. Required: symbol (e.g., 'AAPL'), start_date (YYYY-MM-DD), end_date (YYYY-MM-DD)"
    },
    {
      "parameters": {
        "url": "https://market-data-c60q.onrender.com/api/v1/stock/{{ $json.symbol }}/info",
        "method": "GET",
        "options": {}
      },
      "name": "Get Stock Info",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.1,
      "position": [450, 500],
      "description": "Get detailed stock information. Required: symbol (e.g., 'AAPL')"
    },
    {
      "parameters": {
        "url": "https://market-data-c60q.onrender.com/api/v1/stock/{{ $json.symbol }}/dividends",
        "method": "GET",
        "options": {}
      },
      "name": "Get Dividends",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.1,
      "position": [650, 500],
      "description": "Get dividend history. Required: symbol (e.g., 'AAPL')"
    },
    {
      "parameters": {
        "url": "https://market-data-c60q.onrender.com/api/v1/indicators/list",
        "method": "GET",
        "options": {}
      },
      "name": "List All Indicators",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.1,
      "position": [250, 700],
      "description": "List all available technical indicators with descriptions"
    },
    {
      "parameters": {
        "url": "https://market-data-c60q.onrender.com/api/v1/indicators/{{ $json.symbol }}/{{ $json.indicator }}",
        "method": "GET",
        "sendQuery": true,
        "queryParameters": {
          "parameters": [
            {
              "name": "date",
              "value": "={{ $json.date }}"
            },
            {
              "name": "lookback_days",
              "value": "={{ $json.lookback_days || 10 }}"
            }
          ]
        },
        "options": {}
      },
      "name": "Get Single Indicator",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.1,
      "position": [450, 700],
      "description": "Get specific technical indicator. Required: symbol, indicator (e.g., 'rsi', 'macd'), date (YYYY-MM-DD). Optional: lookback_days (default: 10)"
    },
    {
      "parameters": {
        "url": "https://market-data-c60q.onrender.com/api/v1/indicators/{{ $json.symbol }}/all",
        "method": "GET",
        "sendQuery": true,
        "queryParameters": {
          "parameters": [
            {
              "name": "date",
              "value": "={{ $json.date }}"
            },
            {
              "name": "lookback_days",
              "value": "={{ $json.lookback_days || 10 }}"
            }
          ]
        },
        "options": {}
      },
      "name": "Get All Indicators",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.1,
      "position": [650, 700],
      "description": "Get all technical indicators for a symbol. Required: symbol, date (YYYY-MM-DD). Optional: lookback_days (default: 10)"
